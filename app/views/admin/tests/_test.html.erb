<tr>
  <td class="align-middle">
    <%= content_tag :span, test.title, class: 'test-title', id: 'test', data: { test_id: test.id } %>
    <%= render 'form_inline', test: test == @test ? @test : test %>
  </td>
  <td class="align-middle"><%= test.level %></td>
  <td class="align-middle"><%= test.questions.count %></td>
  <td class="align-middle"><%= link_to I18n.t('admin.show'), admin_test_path(test) %></td>
  <td class="align-middle">
    <%= link_to I18n.t('admin.edit'),
                edit_admin_test_path(test),
                class: 'form-inline-link',
                data: { test_id: test.id } %>
  </td>
  <td class="align-middle">
    <%= link_to I18n.t('admin.delete'),
                admin_test_path(test),
                method: :delete %>
  </td>
</tr>
